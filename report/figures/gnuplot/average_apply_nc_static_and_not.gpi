set term postscript eps color
set output 'avg_static_and_not_apply_nodes.eps'

set xlabel "Benchmarks"
set xtics rotate by -45
set ylabel "Linear avg. per benchmark"
set grid ytics lc rgb "#bbbbbb" lw 1 lt 0

# Plot type
set style data histogram
# Plot type settings
set style fill solid 1.0
set boxwidth 1.0

plot 'benchmark_averages.data' using 5 t "Apply Nodes", \
	'' using 27:xticlabels(1) t "Apply Nodes Calling Statically Known Functions"
